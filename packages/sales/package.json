{
  "name": "@chromind/sales",
  "version": "0.1.0",
  "publishConfig": {
    "access": "public"
  },
  "private": false,
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs",
      "default": "./dist/index.mjs"
    }
  },
  "peerDependencies": {
    "react": ">=18",
    "react-dom": ">=18"
  },
  "peerDependenciesMeta": {
    "@chromind/core": {
      "optional": false
    }
  },
  "devDependencies": {
    "@chromind/core": "workspace:*",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@vitejs/plugin-react": "^4.2.0",
    "rimraf": "^5.0.10",
    "tslib": "^2.6.2",
    "typescript": "^5.4.0",
    "vite": "^5.0.0"
  },
  "scripts": {
    "clean": "rimraf dist",
    "build": "pnpm run clean && vite build && tsc -p tsconfig.build.json"
  }
}
